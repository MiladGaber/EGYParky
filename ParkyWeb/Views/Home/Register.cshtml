@model ParkyWeb.Models.User
@{
    ViewData["Title"] = "Register";
}

<h1>Register</h1>

<form method="post">
    <input type="text" placeholder="Username" asp-for="Username" />
    <input type="password" placeholder="Password" asp-for="Password" />
    <button type="submit" value="Submit" class="btn btn-primary">Register</button>
</form>